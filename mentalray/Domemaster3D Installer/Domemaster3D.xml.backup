                    <name>standalonefiles</name>
                    <platforms>all</platforms>
                    <actionList>
                        <changePermissions>
                            <files>*</files>
                            <permissions>777</permissions>
                        </changePermissions>
                    </actionList>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>installer files/Domemaster3D/mr-standalone</origin>
                        </distributionDirectory>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Domemaster3D Source Images</description>
                    <destination>${installdir}</destination>
                    <name>sourceimages</name>
                    <platforms>all</platforms>
                    <actionList>
                        <addDirectoryToPath>
                            <insertAt>end</insertAt>
                            <path>${installdir}</path>
                        </addDirectoryToPath>
                        <changePermissions>
                            <files>*</files>
                            <permissions>777</permissions>
                        </changePermissions>
                    </actionList>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>installer files/Domemaster3D/sourceimages</origin>
                        </distributionDirectory>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Source Code</description>
                    <destination>${installdir}</destination>
                    <name>sourcecode</name>
                    <platforms>all</platforms>
                    <actionList>
                        <changePermissions>
                            <files>*</files>
                            <permissions>777</permissions>
                        </changePermissions>
                    </actionList>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>installer files/Domemaster3D/sources</origin>
                        </distributionDirectory>
                    </distributionFileList>
                </folder>
            </folderList>
            <startMenuShortcutList>
                <startMenuShortcut>
                    <comment>Uninstall ${product_fullname}</comment>
                    <name>Uninstall ${product_fullname}</name>
                    <runAsAdmin>0</runAsAdmin>
