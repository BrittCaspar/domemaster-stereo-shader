<project>
    <shortName>Domemaster3D</shortName>
    <fullName>Domemaster3D Shader</fullName>
    <version>1.5.3</version>
    <readmeFile>Domemaster3D Readme-Short.txt</readmeFile>
    <leftImage>vortex_sidebar.png</leftImage>
    <splashImage>splash_screen.png</splashImage>
    <componentList>
        <component>
            <name>default</name>
            <description>Default Component</description>
            <canBeEdited>0</canBeEdited>
            <selected>1</selected>
            <show>0</show>
            <folderList>
                <folder>
                    <description>Program Files</description>
                    <destination>${installdir}</destination>
                    <name>programfiles</name>
                    <platforms>all</platforms>
                    <shortcutList>
                        <shortcut>
                            <comment>Uninstall</comment>
                            <exec>${installdir}/${uninstallerName}</exec>
                            <icon></icon>
                            <name>Uninstall ${product_fullname}</name>
                            <path>${installdir}</path>
                            <platforms>all</platforms>
                            <runAsAdmin>0</runAsAdmin>
                            <runInTerminal>0</runInTerminal>
                            <windowsExec>${installdir}/${uninstallerName}.exe</windowsExec>
                            <windowsExecArgs></windowsExecArgs>
                            <windowsIcon></windowsIcon>
                            <windowsPath>${installdir}</windowsPath>
                        </shortcut>
                    </shortcutList>
                </folder>
                <folder>
                    <description>Program Files</description>
                    <destination>${installdir}</destination>
                    <name>programfileslinux</name>
                    <platforms>linux</platforms>
                    <actionList>
                        <changePermissions>
                            <files>*</files>
                            <permissions>777</permissions>
                        </changePermissions>
                    </actionList>
                </folder>
                <folder>
                    <description>Program Files</description>
                    <destination>${installdir}</destination>
                    <name>programfileswindows</name>
                    <platforms>windows</platforms>
                </folder>
                <folder>
                    <description>Program Files</description>
                    <destination>${installdir}</destination>
                    <name>programfilesosx</name>
                    <platforms>osx</platforms>
                    <actionList>
                        <changePermissions>
                            <files>*</files>
                            <permissions>777</permissions>
                        </changePermissions>
                    </actionList>
                </folder>
                <folder>
                    <description>Bin Utilities</description>
                    <destination>${installdir}</destination>
                    <name>bin</name>
                    <platforms>windows</platforms>
                    <actionList>
                        <addDirectoryToPath>
                            <insertAt>end</insertAt>
                            <path>${installdir}/bin</path>
                        </addDirectoryToPath>
                    </actionList>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>installer files/Domemaster3D/bin</origin>
                        </distributionDirectory>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Documentation</description>
                    <destination>${installdir}</destination>
                    <name>docs</name>
                    <platforms>all</platforms>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>installer files/Domemaster3D/docs</origin>
                        </distributionDirectory>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Web Links</description>
                    <destination>${installdir}/docs</destination>
                    <name>links</name>
                    <platforms>all</platforms>
                    <shortcutList>
                        <linkShortcut>
                            <comment>View the latest content on the Domemaster Stereo Shader Wiki</comment>
                            <icon></icon>
                            <name>Domemaster Stereo Wiki</name>
                            <platforms>all</platforms>
                            <runAsAdmin>0</runAsAdmin>
                            <runInTerminal>0</runInTerminal>
                            <url>https://github.com/zicher3d-org/domemaster-stereo-shader/wiki/_pages</url>
                            <windowsIcon></windowsIcon>
                        </linkShortcut>
                        <linkShortcut>
                            <comment>View the latest discussion on the Domemaster Stereo Shader NING Group</comment>
                            <icon></icon>
                            <name>Fulldome NING Group</name>
                            <platforms>all</platforms>
                            <runAsAdmin>0</runAsAdmin>
                            <runInTerminal>0</runInTerminal>
                            <url>http://fulldome.ning.com/forum/topics/stereoscopic-domemaster-images</url>
                            <windowsIcon></windowsIcon>
                        </linkShortcut>
                    </shortcutList>
                </folder>

                <folder>
                    <description>Max Files</description>
                    <destination>${installdir}</destination>
                    <name>maxfiles</name>
                    <platforms>all</platforms>
                    <actionList>
                        <changePermissions>
                            <files>*</files>
                            <permissions>777</permissions>
                        </changePermissions>
                    </actionList>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>installer files/Domemaster3D/max</origin>
                        </distributionDirectory>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Maya Files</description>
                    <destination>${installdir}</destination>
                    <name>mayafiles</name>
                    <platforms>all</platforms>
