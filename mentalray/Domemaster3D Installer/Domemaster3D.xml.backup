                        </changePermissions>
                    </actionList>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>installer files/Domemaster3D/maya</origin>
                        </distributionDirectory>
                    </distributionFileList>
                </folder>
                <folder>
